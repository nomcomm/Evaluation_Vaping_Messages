df_total[c("Experimental_Group", "Participant", "AI_MSG_Rank_mean", "Hum_MSG_Rank_mean")]
df_total_KrusWall_pivot <-
df_total_KrusWall |>
pivot_longer(!c(Experimental_Group, Participant), names_to="AI_or_Hum", values_to="Rank")
